{
  "name": "@signalwire/js",
  "version": "1.0.0",
  "description": "Relay SDK for JavaScript to connect to SignalWire.",
  "author": "SignalWire Team <open.source@signalwire.com>",
  "main": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/esm/js/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean-build": "rm -rf dist/* && npm run build",
    "build": "npm run build-es5 && npm run build-esm",
    "build-es5": "webpack -p",
    "watch-es5": "webpack-dev-server --host 0.0.0.0 --mode development",
    "build-esm": "tsc",
    "watch-esm": "tsc -w",
    "tslint": "tslint -p tsconfig.json",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "npm run test -- --watchAll",
    "validate": "npm i && npm run tslint && npm run test && npm run clean-build"
  },
  "keywords": [
    "signalwire",
    "relay",
    "webrtc",
    "sip",
    "freeswitch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/signalwire-node"
  },
  "bugs": {
    "url": "https://github.com/signalwire/signalwire-node/issues"
  },
  "homepage": "https://github.com/signalwire/signalwire-node",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.6.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/uuid": "^3.4.4",
    "acorn": "^6.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.17.0",
    "typescript": "^3.4.5",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
