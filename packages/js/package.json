{
  "name": "@signalwire/js",
  "version": "1.3.0-alpha.19",
  "description": "Relay SDK for JavaScript to connect to SignalWire.",
  "author": "SignalWire Team <open.source@signalwire.com>",
  "main": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/esm/js/index.js",
  "types": "dist/esm/js/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean-build": "rm -rf dist/* && npm run build",
    "build": "npm run check_versions && npm run build-es5 && npm run build-esm",
    "build-es5": "webpack -p",
    "watch-es5": "webpack-dev-server --host 0.0.0.0 --mode development",
    "build-esm": "tsc",
    "watch-esm": "tsc -w",
    "tslint": "tslint -p tsconfig.json",
    "test": "jest --no-cache --forceExit --detectOpenHandles",
    "test:watch": "npm run test -- --watchAll",
    "validate": "npm i && npm run tslint && npm run test && npm run clean-build",
    "check_versions": "node ../../.scripts/check_package_version.js"
  },
  "keywords": [
    "signalwire",
    "relay",
    "webrtc",
    "sip",
    "freeswitch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/signalwire-node"
  },
  "bugs": {
    "url": "https://github.com/signalwire/signalwire-node/issues"
  },
  "homepage": "https://github.com/signalwire/signalwire-node",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.7.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@types/jest": "^26.0.20",
    "@types/uuid": "^8.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "tslint": "^6.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  }
}
